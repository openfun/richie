{
  "name": "richie-education",
  "version": "2.24.1",
  "description": "A CMS to build learning portals for Open Education",
  "main": "sandbox/manage.py",
  "scripts": {
    "build-ts": "tsc --noEmit && webpack",
    "build-ts-production": "tsc --noEmit && webpack --mode=production",
    "extract-translations": "formatjs extract './**/*.ts*' --ignore ./node_modules --ignore './**/*.d.ts' --out-file './i18n/frontend.json' --id-interpolation-pattern '[sha512:contenthash:base64:6]' --format crowdin",
    "compile-translations": "./i18n/compile-translations.js",
    "lint": "eslint . 'js/**/*.ts?(x)'",
    "prettier-write": "prettier --write 'js/**/*.+(ts|tsx|json|js|jsx)' '*.+(ts|tsx|json|js|jsx)' '**/*.+(css|scss)'",
    "build-sass": "sass scss/_main.scss ../richie/static/richie/css/main.css --load-path=node_modules",
    "build-sass-production": "sass scss/_main.scss ../richie/static/richie/css/main.css --style=compressed --load-path=node_modules",
    "test": "jest",
    "watch-sass": "nodemon -e scss -x 'yarn build-sass'",
    "watch-ts": "yarn build-ts --watch",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build-theme": "cunningham -g scss -o scss/vendors && cunningham -g css -o scss/vendors/css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openfun/richie.git"
  },
  "author": {
    "name": "Open FUN (France Université Numérique)",
    "email": "fun.dev@fun-mooc.fr"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/openfun/richie/issues"
  },
  "homepage": "https://github.com/openfun/richie#readme",
  "browserslist": [
    "last 2 versions",
    ">1%",
    "not dead"
  ],
